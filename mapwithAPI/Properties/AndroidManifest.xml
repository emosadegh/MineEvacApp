<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" android:versionCode="1" android:versionName="1.0" package="com.companyname.googlemappage" android:installLocation="auto">
	<!-- Minimum and target SDK versions -->
	<uses-sdk android:minSdkVersion="19" android:targetSdkVersion="33" />
	<!-- Google Maps for Android v2 requires OpenGL ES v2 -->
	<uses-feature android:glEsVersion="0x00020000" android:required="true" />
	<uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
	<uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
	<uses-permission android:name="android.permission.INTERNET" />
	<uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
	<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
	<uses-permission android:name="com.google.android.providers.gsf.permission.READ_GSERVICES" />
	<uses-permission android:name="com.companyname.googlemappage.permission.MAPS_RECEIVE" />
	<permission android:name="com.companyname.googlemappage.permission.MAPS_RECEIVE" android:protectionLevel="signature" />
	<application android:label="MineEvacEhsan" android:icon="@mipmap/mine_app_icon">
		<activity android:name=".MainActivity" android:label="@string/app_name" android:theme="@style/AppTheme">
			<!-- Other activity attributes -->
		</activity>
		<!-- Put your Google Maps V2 API Key here. -->
		<!-- Google Play Services version -->
		<meta-data android:name="com.google.android.gms.version" android:value="@integer/google_play_services_version" />
		<meta-data android:name="com.google.android.maps.v2.API_KEY" android:value="@string/google_maps_key" />
		<!-- Necessary for apps that target Android 9.0 or higher -->
		<uses-library android:name="org.apache.http.legacy" android:required="false" />
	</application>
</manifest>